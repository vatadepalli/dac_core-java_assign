package com.adikaplabs.core;

public class Employee {
	public static int idCounter;
	static {
		idCounter = 100;
	}
	
	private String name, email, deptId;
	double basic;
	private int id;
	
	public Employee(String name, String email, String deptId, double basic, int id) {
		super();
		this.name = name;
		this.email = email;
		this.deptId = deptId;
		this.basic = basic;
		id = idCounter++;
	}
	
	

}
